{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: FastAPI",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/venv/bin/uvicorn",
        "args": [
          "main:app",
          "--reload",
          "--host",
          "127.0.0.1",
          "--port",
          "8000"
        ],
        "console": "integratedTerminal",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "jinja": true
      }
    ]
  }
  